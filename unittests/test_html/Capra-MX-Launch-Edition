//GA
$j(document).ready(function() {
  sidebarAddLinks();
  sidebarGATrackViews();
  sidebarGATrackClicks();
});

var sectionLabel = getGASectionLabel();

function sidebarAddLinks() {
  $j('.sidebarDeals a').each(function() {
    $j(this).attr('href', $j(this).attr('data-link'));
    $j(this).on('click', function(){
      var trackLink = $j(this).attr('data-track-link');
      if (trackLink) $j.getJSON(trackLink);
    });
  });
}

function sidebarGATrackViews() {
  trackOnVisible('.sidebarDeals li:first', function() {
    __trackGAEvent('Deals',  'Loaded', 'Deals Module - ' + sectionLabel + ' - Sidebar:Above Ad');
  });
}

function sidebarGATrackClicks() {
  $j('.sidebarDeals ul li a').on('click', function(){
    __trackGAEvent('Deals', 'Link Clicked', 'Deals Module - ' + sectionLabel + ' - Sidebar:Above Ad');
  });
}

function getGASectionLabel() {
  var result = null;
  var controllerName = 'products';
  switch(controllerName) {
    case 'homepages':
      result = 'Homepage';
      break;
    case 'products':
      result = 'Product';
      break;
    case 'setup_checks':
      result = 'Bike Check';
      break;
    case 'forum_topics':
      result = 'L3';
      break;
    case 'stories':
      if(['feature', 'product_blog'].includes('')) {
        result = 'Features';
      } else {
        result = 'News';
      }
      break;
    default:
      result = null;
  }

  return result;
}
</script>


