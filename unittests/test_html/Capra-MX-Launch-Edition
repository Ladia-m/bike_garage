    <input type="submit" name="commit" value="Update" role="update-btn" /><input type="submit" name="commit" value="Save as Unpublished" role="draft-btn" /><input type="submit" name="commit" value="Publish" role="publish-btn" /><input type="submit" name="commit" value="Publish as Scheduled" role="schedule-btn" />
    <span role="notice-placeholder"></span>
  </div>

  <input type="hidden" name="publish" id="publish" value="" role="publish-value" />
</div>


</div>
</dd>
</div>
