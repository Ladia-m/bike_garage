  <script>

    $j(document).ready(function() {
    $j('#browseBrandSearch').on('keyup', function() {
      $j('.brandsListUl li').not('.brandsListUl li.browseBrandSearchBox').each(function() {
        currentVal = $j('#browseBrandSearch').val().toLowerCase();
        target = $j(this).find('a').html().toLowerCase();
        if(target.indexOf(currentVal) !== -1){
          $j(this).show();
          //console.log('Brand: '+target+' | Searched: '+currentVal);
        } else {
          $j(this).hide();
        }
      });
    });
    $j('.brandsListUl').hover(
      function() {
        $j(this).css('display','block');
      }, function() {
        $j(this).attr('style','');
      }
    );
    });

  </script>

