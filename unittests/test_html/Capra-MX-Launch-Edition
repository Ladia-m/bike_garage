$j('.moreProducts a').click(function() {
  thisSlot = $j(this).closest('li').index()+1;
  __trackGAEvent('Internal Content Referrals - Clicks', 'More Products', 'Below Product:Slot '+thisSlot, {dimension4:$j(this).attr('href')});
