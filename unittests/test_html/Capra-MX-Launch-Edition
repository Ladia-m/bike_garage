//Content Link Tracking
$j(document).ready(function() {
  $j('.story_body a').click(function() {
    if(!$j(this).hasClass('fancybox')){      
      affNet = $j(this).attr('data-affiliate-network');
      affMerch = $j(this).attr('data-affiliate-merch');
      if(affNet && affMerch && affanLoaded == true){

        __trackGAEvent('Affiliate Actions', 'Article Link Clicked', 'Article');
        __trackGAEvent('Affiliate Networks', affNet, 'Article');
        __trackGAEvent('Affiliate Merchants', affMerch, 'Article');       

      }
    }
  });  

  var affiliateAnalytics__ContentLink;
  var aff_link_data = {};

  if (window.affanLoaded == undefined) affanLoaded = false;

  var affLinkTimer = setInterval(function() {
    if(affanLoaded == true){

      clearInterval(affLinkTimer);

      $j('.story_body a').each(function(index, element) {       

        affNet = $j(this).attr('data-affiliate-network');
        affMerch = $j(this).attr('data-affiliate-merch');

        if(affNet && affMerch && affanLoaded == true && $j(this).hasClass('fancybox') == false){

          $j(this).wrap('<span id="affContainer'+index+'" />');
          $j(this).attr('role', 'affan_link').attr('target','_blank').attr('data-link-class', 'ProductLinkBrowse');

          linkType = "Text";
          if($j(this).find('img').length == 1) linkType = "Image";
          pageUrl = window.location.href;
          publishedAt = $j('.author_info:first .timestamp').attr('data-orig-date');
          reqPath = window.location.pathname;
          sessionId = Affan.sessionId();
          userId = Affan.uniqueId();
          thisSite = "MTB";
          thisTitle = $j(this).html();
          if($j(this).find('img').length == 1){
            if($j(this).find('img').attr('alt')){
              thisTitle = $j(this).find('img').attr('alt');
            } else {
              thisTitle = 'Image';
            }
          }   

          affiliateAnalytics__ContentLink = new Affan(aff_link_data, {
            product_type: 'Article',
            section:      "Article",
            page_url:     pageUrl,
            link_type:    linkType,
            network:      affNet,
            page_url:     pageUrl,
            product_type: "Article",
            published_at: publishedAt,
            request_path: reqPath,
            retailer:     affMerch,
            section:      "Article",
            session_uid:  sessionId,
            sha:          userId,
            site:         thisSite,
            title:        thisTitle,
            user_uid:     userId,
            is_mobile:    0
          });

          affiliateAnalytics__ContentLink.init('#affContainer'+index+'');

        } //if aff link
      });
    }
  }, 500);


  trackMainMenuDesktop();

  //New User
  if($j('.newUserTrig').length > 0){
    if(getFbCookie('fb_connected_guest') == "true"){
      deleteFbCookie('fb_connected_guest');
      __trackGAEvent('New Members', 'Facebook for Registration');
      __trackGAEvent('Facebook Connected Members', 'Facebook For Registration');
    } else {
      __trackGAEvent('New Members', 'Direct Registration');
    }
  }

  //FB Connect
  if(getFbCookie('fb_connected_user') == "true"){
    deleteFbCookie('fb_connected_user');
    __trackGAEvent('Facebook Connected Members', 'Connect Existing Profile');
  } 

});

function getFbCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}
function deleteFbCookie(cname) {
  document.cookie = cname + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
}
