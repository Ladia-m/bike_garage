<script charset="utf-8">
  $j(document).ready(function() {
  $j.getJSON("/mm_config?manufacturer=1088", function(data){
    if (data.length > 0){
      $j('.merchants_wrapper tbody tr').hide();
      $j('.merchants_wrapper tbody tr').attr('data-hidden', 'true');
      $j(data).each(function(){
        $j('.merchants_wrapper tbody tr[data-merchant-id='+this+']').show();
        $j('.merchants_wrapper tbody tr[data-merchant-id='+this+']').attr('data-hidden', 'false');
      });
      $j('.merchants_wrapper').each(function(){
        var ind = 0;
        $j(this).find('tr[data-merchant-id][data-hidden=false]').each(function(){
          ind += 1;
          $j(this).find('[data-pos]').attr('data-pos', ind);
        });
      });
      if ($j('#links_collapsed tbody tr[data-hidden=false]').length == $j('#links_expanded tbody tr[data-hidden=false]').length){
        $j('#links_collapsed tfoot tr').hide();
      }
      $j('.merchants_wrapper').show();
      var affTimer1 = setInterval(function() {
        if(affanLoaded == true && typeof bindViewEvents == 'function'){
          clearInterval(affTimer1);
          affiliateAnalytics_upper_guide_module.bindViewEvents();
        }
      }, 500);
    }
  });
  });

  $j(document).ready(function () {
    var productId = $j(".b-product-specs").data("id");

    $j.ajax("/spc/"+productId, {
      dataType: "json",
      type: "GET",
      success: function (result) {
        $j(".product_views b").after(result.views);
        $j(".product_views").css('visibility', 'visible');
      }
    });

    $j('#search_query').on('change', function(e){
      var form = $j('#productSearchForm');
      var action = $j(this).attr('action');
      var query = $j(this).val();
      form.attr('action', productSearchUrl(query));
    });
  });
</script>

