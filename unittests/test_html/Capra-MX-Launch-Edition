<script>

var abActive = window.justDetectAdblock == undefined;
if (window.justDetectAdblock){
  justDetectAdblock.detectAnyAdblocker().then(function(detected) {
    abActive = detected;
  });
}
setTimeout(function(){
  if (!abActive){
    //alert('You are not blocking ads!');
    window.prerollFinished = false
  } else {
    //alert('You are blocking ads!');
    window.prerollFinished = true
    $j(document).ready(function() {
      __trackGAEvent('Adblocking', 'Adblocking - Yes');
    });
  }
}, 500);
</script>

