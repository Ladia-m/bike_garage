$j(document).ready(function() {
  footerAddLinks();
  footerGATrackViews();
  footerGATrackClicks();
});

var sectionLabel = getGASectionLabel();

function footerAddLinks() {
  $j('.footerProducts a').each(function() {
    $j(this).attr('href', $j(this).attr('data-link'));
    $j(this).on('click', function(){
      var trackLink = $j(this).attr('data-track-link');
      if (trackLink) $j.getJSON(trackLink);
    });
  });
}

function footerGATrackViews() {
  trackOnVisible('.footerProducts li:first', function() {
    __trackGAEvent('Deals',  'Loaded', 'Deals Module - ' + sectionLabel + ' - Footer:Middle');
  });
}

function footerGATrackClicks() {
  $j('.footerProducts ul li a').on('click', function(){
    __trackGAEvent('Deals', 'Link Clicked', 'Deals Module - ' + sectionLabel + ' - Footer:Middle');
  });
}

function getGASectionLabel() {
  var result = null;
  var controllerName = 'products';
  switch(controllerName) {
    case 'homepages':
      result = 'Homepage';
      break;
    case 'products':
      result = 'Product';
      break;
    case 'setup_checks':
      result = 'Bike Check';
      break;
    case 'forum_topics':
      result = 'L3';
      break;
    default:
      result = null;
  }

  return result;
}
</script>


<script type='text/javascript'>
	
	/*Swap Image Size*/
	$j(document).ready(function() {
	  relatedFooterImageSwap();
	});	
	$j(window).resize(function() {
	  relatedFooterImageSwap();
	});
	function relatedFooterImageSwap() {
	  $j('.related_in_footer li .photo img').each(function() {
	    imgSize = $j(this).closest('li').width();
	    console.log('imgSize:'+imgSize);
	    origSize = '/c235x132_';
	    originalURL = $j(this).attr('data-original');
	    if(!originalURL){
	    	originalURL = $j(this).attr('src');
	    	$j(this).attr('data-original', originalURL);
	    }
	    if(imgSize > 500){	
	  		newUrl = originalURL.replace(origSize, "/c600x337_"); 
	  		$j(this).attr('src', newUrl);  	
	  	} else if (imgSize > 366) { 
	  		newUrl = originalURL.replace(origSize, "/c500x280_"); 
	  		$j(this).attr('src', newUrl);  		
	  	} else if (imgSize > 235) { 
	  		newUrl = originalURL.replace(origSize, "/c366x206_"); 
	  		$j(this).attr('src', newUrl);
	  	} else {
	  	  newUrl = originalURL.replace(origSize, "/c235x132_"); 
	  	  $j(this).attr('src', newUrl);
	    }
	  });
	}	
</script>


<script type='text/javascript'>
	
	/*Swap Image Size*/
